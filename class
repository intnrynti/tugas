class sekolah :
     pass
   
sekolah1 = sekolah()
sekolah2 = sekolah()

print(type(sekolah1))
print(type(sekolah2))

sekolah1 = sekolah()
sekolah1.nama = "SMK PGRI 35 Solokan Jeruk"
sekolah1.alamat = "JL RHO Kosasih No 90 Cibodas"

sekolah2 = sekolah()
sekolah2.nama = "SMKN 9 Bandung"
sekolah2.alamat = "JL Soekarno-Hatta No 10"

print(sekolah1.nama)
print(sekolah1.alamat)
print(sekolah2.nama)
print(sekolah2.alamat)